<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Membership1Controller">
    <children>
        <ImageView fitHeight="45.0" fitWidth="150.0" layoutX="278.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/gymLogo.png" />
            </image>
        </ImageView>
        <Label layoutX="284.0" layoutY="90.0" text="Great!" />
        <Label layoutX="31.0" layoutY="107.0" text="Now that you have completed your registration, let`s get you started by creating your MEMBERSHIP!" />
        <Label layoutX="84.0" layoutY="176.0" text="Select the gym you want to go:" />
        <ComboBox fx:id="comboBoxSelectGym" layoutX="260.0" layoutY="172.0" onAction="#keyReleasedProperty" prefWidth="150.0" />
        <Separator layoutX="191.0" layoutY="204.0" prefHeight="3.0" prefWidth="318.0" />
        <Label layoutX="150.0" layoutY="217.0" text="Select your trainer:" />
        <Separator layoutX="191.0" layoutY="245.0" prefHeight="3.0" prefWidth="318.0" />
        <Label layoutX="170.0" layoutY="258.0" text="Select the diet:" />
        <ComboBox fx:id="comboBoxSelectTrainer" layoutX="260.0" layoutY="213.0" onAction="#keyReleasedProperty" prefWidth="150.0" />
        <ComboBox fx:id="comboBoxSelectDiet" layoutX="260.0" layoutY="254.0" onAction="#keyReleasedProperty" prefWidth="150.0" />
        <Label layoutX="108.0" layoutY="300.0" text="Please select the exercises:" />
        <ComboBox fx:id="comboBoxSelectEx" layoutX="259.0" layoutY="296.0" onAction="#keyReleasedProperty" prefWidth="150.0" />
        <Button fx:id="buttonFinish" layoutX="515.0" layoutY="361.0" mnemonicParsing="false" onAction="#onClickButtonFinish" text="Finish" />
        <Separator layoutX="191.0" layoutY="286.0" prefHeight="3.0" prefWidth="318.0" />
    </children>
</AnchorPane>
