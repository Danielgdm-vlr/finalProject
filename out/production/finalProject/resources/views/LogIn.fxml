<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.LogInController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="username" layoutX="226.0" layoutY="157.0" onKeyReleased="#keyReleasedProperty" promptText="Username" />
        <PasswordField fx:id="password" layoutX="226.0" layoutY="207.0" onKeyReleased="#keyReleasedProperty" promptText="Password" />
        <CheckBox fx:id="checkBoxVerifyHuman" layoutX="195.0" layoutY="258.0" mnemonicParsing="false" onAction="#keyReleasedProperty" prefHeight="17.0" prefWidth="208.0" text="Please verify that you are a human." />
        <Button fx:id="buttonLogIn" layoutX="428.0" layoutY="182.0" mnemonicParsing="false" onAction="#onClickButtonLogIn" prefHeight="22.0" prefWidth="71.0" text="LOG IN!" />
        <Label layoutX="525.0" layoutY="327.0" text="OR:" />
        <Button fx:id="buttonSignUp" layoutX="499.0" layoutY="344.0" mnemonicParsing="false" onAction="#onClickButtonSignUp" prefHeight="25.0" prefWidth="71.0" text="SIGN UP!" />
        <ImageView fitHeight="94.0" fitWidth="149.0" layoutX="252.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/gymLogo.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="25.0" fitWidth="150.0" layoutX="193.0" layoutY="157.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/user.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="25.0" fitWidth="19.0" layoutX="196.0" layoutY="209.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/password.png" />
            </image>
        </ImageView>
        <Label layoutX="420.0" layoutY="78.0" text="RAW POWER" />
        <Label layoutX="348.0" layoutY="95.0" text="BECOME THE BEST VERSION OF YOURSELF!" />
    </children>
</AnchorPane>
