        <Separator layoutX="157.0" layoutY="187.0" prefHeight="3.0" prefWidth="318.0" />
        <Label layoutX="116.0" layoutY="200.0" text="Select your trainer:" />
        <Separator layoutX="157.0" layoutY="228.0" prefHeight="3.0" prefWidth="318.0" />
        <Label layoutX="136.0" layoutY="243.0" text="Select the diet:" />
